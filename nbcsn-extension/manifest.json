{
    "manifest_version": 2,

    "name": "Full Screen NBCSN",
    "description": "Watch NBCSN full screen without the ad.",
    "version": "1.0.32",

    "content_scripts": [
    {
        "matches": ["http://stream.nbcsports.com/*/*", "http://stream.nbcsports.com/*", "http://stream.golfchannel.com/*", "http://stream.nbcdeportes.com/*"],
        "js": ["nbcsn.js"],
        "css": ["nbcsn.css"],
        "all_frames": true
    }
    ]
}